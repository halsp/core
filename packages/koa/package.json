    "koa-compose": "^4.1.0",
    "query-string": "^7.1.1"
  },
  "devDependencies": {
    "@koa/cors": "4.0.0",
    "@koa/router": "^12.0.0",
    "@types/koa": "^2.13.5",
    "@types/koa-cors": "0.0.2",
    "@types/koa__cors": "^3.3.0",
    "@types/koa__router": "^12.0.0",
    "@types/supertest": "^2.0.12",
    "supertest": "^6.3.1"
  },
  "files": [
    "/dist/*",
    "/src/*",
    "/test/*",
    ".eslintignore",
    ".eslintrc.js",
    ".gitignore",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
