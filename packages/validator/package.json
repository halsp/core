{
  "name": "@ipare/validator",
  "version": "3.1.1",
  "description": "Ipare 的参数校验插件",
  "engines": {
    "node": ">=12.14.0"
  },
  "homepage": "https://ipare.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipare/ipare"
  },
  "keywords": [
    "ipare",
    "pipe",
    "class-validator"
  ],
  "bugs": {
    "url": "https://github.com/ipare/ipare/issues",
    "email": "support@hal.wang"
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "rimraf ./dist/* && tsc",
    "lint": "eslint src test --ext .ts",
    "pretest": "npm run build",
    "test": "jest"
  },
  "author": "hal-wang",
  "license": "MIT",
  "dependencies": {
    "@ipare/core": "^3.1.1",
    "@ipare/inject": "^3.1.1",
    "@ipare/pipe": "^3.1.1",
    "class-validator": "^0.13.2",
    "reflect-metadata": "^0.1.13"
  },
  "peerDependencies": {
    "class-validator": "*"
  },
  "peerDependenciesMeta": {
    "class-validator": {
      "optional": true
    }
  },
  "files": [
    "/dist/*",
    "/src/*",
    "/test/*",
    ".eslintignore",
    ".eslintrc.js",
    ".gitignore",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
