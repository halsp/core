{
  "name": "@ipare/lambda",
  "version": "3.0.3",
  "description": "将 Ipare 托管到 serverless 云函数，如腾讯云云函数、阿里云云函数、awa lambda、azure functions 等",
  "engines": {
    "node": ">=12.14.0"
  },
  "homepage": "https://ipare.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipare/ipare"
  },
  "keywords": [
    "ipare",
    "lambda",
    "serverless",
    "cloudbase",
    "azure functions",
    "awa lambda",
    "function compute"
  ],
  "bugs": {
    "url": "https://github.com/ipare/ipare/issues",
    "email": "support@hal.wang"
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "rimraf ./dist/* && tsc",
    "lint": "eslint src test --ext .ts",
    "pretest": "npm run build",
    "test": "jest"
  },
  "author": "hal-wang",
  "license": "MIT",
  "dependencies": {
    "@ipare/core": "^3.0.3",
    "@ipare/http": "^3.0.3",
    "@ipare/methods": "^3.0.3"
  },
  "devDependencies": {
    "@types/shelljs": "^0.8.11"
  },
  "files": [
    "/dist/*",
    "/src/*",
    "/test/*",
    ".eslintignore",
    ".eslintrc.js",
    ".gitignore",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
