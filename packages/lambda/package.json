{
  "name": "@halsp/lambda",
  "version": "2.2.0",
  "description": "将 Halsp 托管到 serverless 云函数，如腾讯云云函数、阿里云云函数、awa lambda、azure functions 等",
  "engines": {
    "node": ">=12.14.0"
  },
  "homepage": "https://halsp.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/halsp/core"
  },
  "keywords": [
    "halsp",
    "lambda",
    "serverless",
    "cloudbase",
    "azure functions",
    "awa lambda",
    "function compute"
  ],
  "bugs": {
    "url": "https://github.com/halsp/core/issues",
    "email": "support@hal.wang"
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "sh ../../scripts/clean-dist.sh && tsc",
    "lint": "eslint src test --ext .ts",
    "pretest": "npm run build",
    "test": "jest"
  },
  "author": "hal-wang",
  "license": "MIT",
  "dependencies": {
    "@halsp/core": "^2.2.0",
    "@halsp/http": "^2.2.0"
  },
  "devDependencies": {
    "@types/shelljs": "^0.8.12"
  },
  "files": [
    "/dist/*",
    "/src/*",
    "/test/*",
    ".eslintignore",
    ".eslintrc.js",
    ".gitignore",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
