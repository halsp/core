{
  "name": "@ipare/static",
  "version": "3.1.0",
  "description": "Ipare 的静态资源插件",
  "engines": {
    "node": ">=12.14.0"
  },
  "homepage": "https://ipare.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/ipare/ipare"
  },
  "keywords": [
    "ipare",
    "static-file",
    "static-website"
  ],
  "bugs": {
    "url": "https://github.com/ipare/ipare/issues",
    "email": "support@hal.wang"
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "rimraf ./dist/* && tsc",
    "lint": "eslint src test --ext .ts",
    "pretest": "npm run build",
    "test": "jest"
  },
  "author": "hal-wang",
  "license": "MIT",
  "dependencies": {
    "@ipare/core": "^3.1.0",
    "@ipare/http": "^3.1.0",
    "@ipare/methods": "^3.1.0",
    "glob": "^8.0.3",
    "mime": "^3.0.0"
  },
  "devDependencies": {
    "@types/glob": "^8.0.0",
    "@types/mime": "^3.0.1"
  },
  "files": [
    "/dist/*",
    "/html/*",
    "/src/*",
    "/test/*",
    ".eslintrc.js",
    ".gitignore",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
