{
  "name": "@halsp/testing",
  "version": "1.0.0",
  "description": "Halsp 的测试插件，用于单元测试、本地测试等",
  "engines": {
    "node": ">=12.14.0"
  },
  "homepage": "https://halsp.org",
  "repository": {
    "type": "git",
    "url": "https://github.com/halsp/core"
  },
  "keywords": [
    "halsp",
    "test"
  ],
  "bugs": {
    "url": "https://github.com/halsp/core/issues",
    "email": "support@hal.wang"
  },
  "private": false,
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "dist/index.js",
  "scripts": {
    "prebuild": "npm run lint",
    "build": "rimraf ./dist/* && tsc",
    "lint": "eslint src test --ext .ts",
    "pretest": "npm run build",
    "test": "jest"
  },
  "author": "hal-wang",
  "license": "MIT",
  "dependencies": {
    "@halsp/common": "^1.0.0"
  },
  "devDependencies": {
    "@halsp/http": "^1.0.0",
    "@halsp/micro": "^1.0.0",
    "@halsp/micro-client": "^1.0.0",
    "@halsp/micro-common": "^1.0.0",
    "@halsp/micro-grpc": "^1.0.0",
    "@halsp/micro-mqtt": "^1.0.0",
    "@halsp/micro-nats": "^1.0.0",
    "@halsp/micro-redis": "^1.0.0",
    "@halsp/micro-tcp": "^1.0.0",
    "@halsp/native": "^1.0.0",
    "supertest": "^6.3.3"
  },
  "files": [
    "/dist/*",
    "/src/*",
    "/test/*",
    ".eslintignore",
    ".eslintrc.js",
    ".gitignore",
    "LICENSE",
    "package.json",
    "README.md",
    "tsconfig.json"
  ]
}
